{
  "presets": ["@babel/env", "@babel/preset-react", [
    "@nrwl/react/babel",
    {
      "runtime": "automatic"
    }
  ]],
  "plugins": [
    ["@babel/plugin-proposal-private-property-in-object", {
      "loose": true
    }],
    ["@babel/plugin-proposal-private-methods", {
      "loose": true
    }]
  ]
}
